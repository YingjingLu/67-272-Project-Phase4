<%= simple_form_for(@order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :school_id %>
    <%= f.input :user_id %>
    <%= f.input :date %>
    <%= f.input :grand_total %>
    <%= f.input :payment_receipt %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
